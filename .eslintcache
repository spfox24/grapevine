m '../../img/gvlogo.png';\n\nconst BASE_URL = 'http://localhost:3001/api/users';\n\nfunction EditForm(props) {\n    \n    const [ editState, setEditState ] = useState({\n        title: \"\",\n        content: \"\",\n    });\n    \n    async function handleSubmit(evt) {\n\n    \n        const itemUrl = props.location.pathname.split('/')\n            console.log(itemUrl)\n        const itemId = itemUrl[2];\n            \n        try {\n            evt.preventDefault();\n            await updateList(editState, itemId);\n\n            props.history.push('/dashboard');\n\n        } catch (error) {\n            alert(error.message);\n        }\n    }\n\n    function handleChange(input, arg) {\n        setEditState(prevState => ({\n            ...prevState,\n            [input.name]: arg,\n        }));\n    }\n\n    function handleContentChange(evt) {\n        const value = evt.target.value;\n\n        setEditState(prevState => ({\n            ...prevState,\n            content: value,\n        }))\n    }\n\n    useEffect(() => {\n    \n        const userId = getUser()._id;\n\n        const requestOptions = {\n            headers: { \n                'Content-Type': 'Application/json',\n                'Authorization': 'Bearer ' + getToken()\n            }\n        }\n\n        const itemUrl = props.location.pathname.split('/')\n        const itemId = itemUrl[2];\n        \n\n        fetch(`${BASE_URL}/edit/${itemId}/${userId}`, requestOptions)\n        .then(response => response.json())\n        .then(data => setEditState({ title: data.listItem.title, content: data.listItem.content }))\n    }, []);\n\n    const inputRef = useRef();\n\n    useEffect(() => {\n            inputRef.current.populateEditField(editState.title)\n    }, [editState]);\n\n    return (\n        <div className=\"EditForm-container\">\n            <h1 className=\"list-header\">Edit List</h1>\n            <section className=\"EditForm\">\n                <form className=\"formBody\" onSubmit={handleSubmit}>\n                    <InputField\n                        name=\"title\"\n                        handleChange={handleChange}  \n                        type=\"text\"\n                        placeholder=\"Title\"\n                        ref={inputRef}\n                        value={editState.title}\n                        active={true}\n                        locked={true}\n                    />\n                    <label for=\"Content\"></label>\n                        <div className=\"select\">\n                            <select \n                                className=\"Content-select\" \n                                value={editState.content} \n                                onChange={handleContentChange}>\n                                <option value=\"Movie\">Movie</option>\n                                <option value=\"Show\">Show</option>\n                                <option value=\"Book\">Book</option>\n                                <option value=\"\">Content</option>\n                            </select>\n                            <span className=\"focus\"></span>\n                        </div>\n                        <button>Submit</button>\n                    <div className=\"cancelLink\">\n                        <Link to=\"/dashboard\" className=\"cancel\">Cancel</Link>\n                    </div>\n                </form>\n            </section>\n            <div>\n                <img src={logo} className=\"logo-footer\" alt=\"logo\"/>\n            </div>\n        </div>\n    )\n}\n\nexport default EditForm;","/Users/stevenfox/Desktop/grapevine/grapevine-frontend/src/components/ScrollToTop/ScrollToTop.js",[],"/Users/stevenfox/Desktop/grapevine/grapevine-frontend/src/pages/Movies/MoviesPage.js",[],"/Users/stevenfox/Desktop/grapevine/grapevine-frontend/src/pages/Shows/ShowsPage.js",[],"/Users/stevenfox/Desktop/grapevine/grapevine-frontend/src/pages/Books/BooksPage.js",[],"/Users/stevenfox/Desktop/grapevine/grapevine-f